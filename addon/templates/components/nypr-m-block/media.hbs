{{#link-to @route @model}}
  <NyprAPicture
    @src-s={{@srcS}}
    @break-m={{@breakM}}
    @src-m={{@srcM}}
    @break-l={{@breakL}}
    @src-l={{@srcL}}
    @break-xl={{@breakXL}}
    @src-xl={{@srcXL}}
    @alt={{@alt}}
    @lazy={{@lazy}}
    @width={{@width}}
    @height={{@height}}
  />
{{/link-to}}

{{#if @tag}}
  {{#link-to @tagRoute @tag.model class="c-block__tag o-kicker"}}{{@tag.label}}{{/link-to}}
{{/if}}
